<h1>Create Chirrps account</h1>

<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :insert_img_url %>
  <%= f.text_field :img_url %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :cover %>
  <%= f.text_field :cover %>
    <%= f.label :about %>
  <%= f.text_field :about %>

  <%= f.submit %>
<% end %>